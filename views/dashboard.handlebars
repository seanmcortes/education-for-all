<!DOCTYPE html>
<html>
  <head>
    <link href="styles.css" rel="stylesheet" type="text/css">
    <meta charset="UTF-8">
  </head>
  <body>
    <h2>Dashboard</h2>
      <div>
        <center>
          <nav>
            <ul> 
              <li><b><a href="">Profile</a></b></li>
              <li><b><a href="">Education Planner</a></b></li>
              <li><b><a href="">Education Progress</a></b></li>
              <li><b><a href="/logout">Logout</a></b></li>
            </ul>
          </nav>
        </center>
      </div>

      <div>
        <table id="classes">
          {{#if courseLink}}
            {{#each courseLink}}
              <tr>
                <td>
                  <a href="/course/{{this.course_id}}" id={{this.id}}>
                    {{this.name}}
                  </a>
                </td>
              </tr>
            {{/each}}
          {{/if}}    
        </table>  
      </div>
    <script type="text/javascript" src="/client.js"></script>
  </body>
</html>
